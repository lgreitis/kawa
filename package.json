    "@heroicons/react": "^2.1.5",
    "@lgreitis/kawa-sdk": "^0.0.5",
    "@radix-ui/react-scroll-area": "1.2.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@tanstack/query-sync-storage-persister": "^5.59.16",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.55.4",
    "@tanstack/react-query-persist-client": "^5.59.16",
    "@tanstack/react-virtual": "^3.10.9",
    "@xyflow/react": "^12.3.0",
    "anitomyscript": "github:ThaUnknown/anitomyscript#42290c4b3f256893be08a4e89051f448ff5e9d00",
    "async-lock": "^1.4.1",
    "axios": "^1.7.7",
    "comlink": "^4.4.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.1.7",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "fast-xml-parser": "^5.2.5",
    "framer-motion": "^12.29.2",
    "jassub": "^1.8.6",
    "matroska-metadata": "^1.0.6",
    "pako": "^2.1.0",
    "parse-torrent": "^11.0.17",
    "pretty-bytes": "^7.1.0",
    "react-dropzone": "^14.2.3",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^6.26.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "usehooks-ts": "^3.1.0",
    "video.js": "^8.17.3",
    "videojs-hotkeys": "^0.2.30",
    "webtorrent": "^2.5.10",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/async-lock": "^1.4.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.14.8",
    "@types/pako": "^2.0.3",
    "@types/parse-torrent": "^5.8.7",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/webtorrent": "^0.110.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^1.0.0",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "8.57.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.1.3",
    "postcss": "^8.4.45",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.9.2",
    "vite": "^7.3.1"
  }
